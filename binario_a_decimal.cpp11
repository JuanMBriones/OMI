#include <iostream>

using namespace std;

void b(int n) {
    if(n>=2) {
        b(n >> 1);
        printf("%d\n", n%2);
    }
    else {
        printf("%d\n", n%2);
    }
}

int main() {
    int n;
    cin >> n;
    if(n>=0 && n<=500) {
        b(n);
    }
    else {
        cout << "Error" << endl;
    }
    cin.get();
    return 0;
}
