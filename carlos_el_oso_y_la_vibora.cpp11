int main() {
    //string c(4, 'e'); // eeee
    //cout << c << endl;
    int n, m, i, j;
    cin >> n >> m;
    for(i=0; i<n; i++) {
        if((i+1)%2==1) {
            for(j=0; j<m; j++) {
                cout << "#";
            }
        }
        else {
            if((i+1)%4==0) {
                cout << "#";
                for(j=1; j<m; j++) {
                    cout << ".";
                }
            }
            else {
                for(j=1; j<m; j++) {
                    cout << ".";
                }
                cout << "#";
            }
        }
        cout << endl;
    }
    cin.get();
    return 0;
}