#include <iostream>
#include <vector>

using namespace std;

int main() {
    vector <int>a;
    int b, an;
    cin >> b;
    an = b-1;
    a.push_back(b);
    while(b>an) {
        an = b;
        cin >> b;
        a.push_back(b);
    }
    cout << a.size()-1 << endl;
    cin.get();
    return 0;
}
